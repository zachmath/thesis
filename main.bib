file = {:home/zachary/Dropbox/Mendeley/Schuetrumpf, Nazarewicz, Reinhard - 2017 - Physical Review C - Central depression in nucleonic densities Trend analysis in the nuclear d.pdf:pdf},
issn = {2469-9985},
journal = {Phys. Rev. C},
month = {aug},
number = {2},
pages = {024306},
title = {{Central depression in nucleonic densities: Trend analysis in the nuclear density functional theory approach}},
url = {https://link.aps.org/doi/10.1103/PhysRevC.96.024306},
volume = {96},
