arxivId = {arXiv:1410.1264v1},
author = {Sadhukhan, Jhilam and Dobaczewski, J. and Nazarewicz, W. and Sheikh, J. A. and Baran, A.},
doi = {10.1103/PhysRevC.90.061304},
eprint = {arXiv:1410.1264v1},
file = {:home/zachary/Dropbox/Mendeley/Sadhukhan et al. - 2014 - Physical Review C - Pairing-induced speedup of nuclear spontaneous fission.pdf:pdf},
issn = {0556-2813},
journal = {Phys. Rev. C},
month = {dec},
number = {6},
