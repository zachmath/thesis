doi = {10.1103/PhysRevC.87.024320},
eprint = {arXiv:1208.1215v1},
file = {:home/zachary/Dropbox/Mendeley/Staszczak, Baran, Nazarewicz - 2013 - Physical Review C - Spontaneous fission modes and lifetimes of superheavy elements in the nuclear.pdf:pdf},
