author = {Schunck, N. and Dobaczewski, J. and Satu{\l}a, W. and B{\c{a}}czyk, P. and Dudek, J. and Gao, Y. and Konieczka, M. and Sato, K. and Shi, Y. and Wang, X.B. and Werner, T.R.},
doi = {10.1016/j.cpc.2017.03.007},
