journal = {Phys. Rev. C},
month = {feb},
number = {2},
pages = {024320},
publisher = {American Physical Society},
title = {{Spontaneous fission modes and lifetimes of superheavy elements in the nuclear density functional theory}},
url = {https://link.aps.org/doi/10.1103/PhysRevC.87.024320 /citations?view{\_}op=view{\_}citation{\&}continue=/scholar{\%}3Fhl{\%}3Den{\%}26start{\%}3D10{\%}26as{\_}sdt{\%}3D0,23{\%}26scilib{\%}3D1{\&}citilm=1{\&}citation{\_}for{\_}view=OxIGOuMAAAAJ:d1gkVwhDpl0C{\&}hl=en{\&}oi=p},
